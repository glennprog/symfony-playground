{% extends '::base.html.twig' %}
{% block title %}My Photo{% endblock %}

{% block stylesheetsChild %}
    <link rel="stylesheet" href="{{ asset('application/css/GMPhototequeBundle.css') }}" />
{% endblock %}

{% block javascriptsChild %}
    <script src="{{ asset('application/js/GMPhototequeBundle.js') }}"></script>
{% endblock %}

{% block flashbag %}
    <div class="container margin-top-xs">
        {{parent()}}
    </div>
{% endblock %}

{% block app_bar_menu %}
    {% if not is_granted("IS_AUTHENTICATED_REMEMBERED") %}
        <script>window.location.href = '{{ path("fos_user_security_login") }}'</script>
    {% endif %}
    {% include 'GMPhototequeBundle::navbar_GMPhototequeBundle.html.twig' %}

{% endblock %}