{% if paginator is defined %}
<div id="paginator_{{paginator['entity']}}" class = "margin-bottom-s paginator">
   {% if paginator is defined %}
   <div class="flex-container">
      <div class="paginator-container-prev">
         <a id="paginator_prev_fast_{{paginator['entity']}}" class="text-dark paginator-prev-fast" 
            href={{paginator['paginator_prev_fast']}}><i class="fas fa-fast-backward"></i>
         </a>
         <a id="paginator_prev_{{paginator['entity']}}" class="text-dark paginator-prev" 
            href={{paginator['paginator_prev']}}><i class="fas fa-backward"></i>
         </a>
      </div>
      <div class="paginator-container-next">
         <a id="paginator_next_{{paginator['entity']}}" class="text-dark paginator-next" 
            href={{paginator['paginator_next']}}><i class="fas fa-forward"></i>
         </a>
         <a id="paginator_next_fast_{{paginator['entity']}}" class="text-dark paginator-next-fast" 
            href={{paginator['paginator_next_fast']}}><i class="fas fa-fast-forward"></i>
         </a>
      </div>
      <div class="paginator-container-count">
         <span>{{'Max'|trans}} :</span>
         <select name="paginator_container_count_name_{{paginator['entity']}}" id="paginator_container_count_id_{{paginator['entity']}}">
         <option value="5" {% if paginator['count'] == 5%}selected{% endif %}>5</option>
         <option value="10" {% if paginator['count'] == 10%}selected{% endif %}>10</option>
         <option value="25" {% if paginator['count'] == 25%}selected{% endif %}>25</option>
         <option value="50" {% if paginator['count'] == 50%}selected{% endif %}>50</option>
         <option value="100" {% if paginator['count'] == 100%}selected{% endif %}>100</option>
         <option value="200" {% if paginator['count'] == 200%}selected{% endif %}>200</option>
         </select>
      </div>
      <div class="paginator-container-page">
         Page <input type="text" name="paginator_container_page_name_{{paginator['entity']}}" id="paginator_container_page_id_{{paginator['entity']}}" placeholder="Entrer le numéro de la page" value = {{paginator['current_page']}}>
      </div>
      <div class="paginator-container-page-total">
         {% if paginator['current_page'] <= paginator['total_page'] %}
         <span id="paginator_container_page_total_text_{{paginator['entity']}}">de {{paginator['total_page']}}</span>
         {% endif %}
      </div>
      <div class="paginator-container-next_record_to_read">
         {% if paginator['next_record_to_read'] != null %}
         <span id="paginator_container_next_record_to_read_text_{{paginator['entity']}}">{{'Reste à lire'|trans}} : {{paginator['next_record_to_read']}}</span>
         {% else %}
         <span id="paginator_container_next_record_to_read_text_{{paginator['entity']}}">{{'Reste à lire : zéro'|trans}}</span>
         {% endif %}
      </div>
      <div class="paginator-container-total-entities">
         <span id="paginator_container_total_entities_text_{{paginator['entity']}}">{{'Total global'|trans}} : {{paginator['total_entities']}}</span>
      </div>
   </div>
   {% endif %}
</div>
{% endif %}


